{
    // mermain markdown
    "markdown-preview-enhanced.enableScriptExecution": true,
    "markdown-preview-enhanced.mermaidTheme": "forest",
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "markdown-preview-enhanced.previewTheme": "newsprint.css",
    "markdown-preview-enhanced.enableExtendedTableSyntax": true,
    "markdown-preview-enhanced.enableTypographer": true,
    "markdown.extension.toc.orderedList": true,
    "markdown.extension.orderedList.autoRenumber": false,
    // "terminal.integrated.shell.windows": "C:\\Windows\\System32\\wsl.exe",
    // "terminal.integrated.shell.windows": "C:\\Windows\\System32\\bash.exe",
    // "python.pythonPath": "C:\\Python37\\python.exe",
    "window.zoomLevel": 1,
    "files.eol": "\n",
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": false, //最左边的导航?栏 文件 扩展
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.colorTheme": "One Dark Pro",
    "editor.renderControlCharacters": true,
    "editor.fontSize": 20,
    // "java.errors.incompleteClasspath.severity": "ignore",
    "terminal.integrated.rendererType": "dom",
    "editor.lineNumbers": "off",
    "C_Cpp.loggingLevel": "None",
    "git.confirmSync": false,
    "editor.mouseWheelZoom": true,
    "python.dataScience.maxOutputSize": 500,
    "editor.minimap.enabled": false,
    "python.linting.pylintArgs": [
        "--extension-pkg-whitelist=PyQt5"
    ],
    "python.formatting.yapfArgs": [
        "--style={ based_on_style: google, column_limit: 80 }"
    ],
    // todo tree settings
    "todo-tree.regex.regex": "((//|#|<!--|;|/\\*|^)\\s*($TAGS):|^\\s*- \\[ \\])",
    "todo-tree.general.tags": [
        "TODO",
        "FIXME",
        "tag",
        "done",
        "note"
    ],
    "todo-tree.regex.regexCaseSensitive": false,
    "todo-tree.tree.showInExplorer": true,
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "white",
        "background": "yellow",
        "icon": "check",
        "rulerColour": "yellow",
        "type": "tag",
        "iconColour": "yellow"
    },
    "todo-tree.highlights.customHighlight": {
        "todo": {
            "background": "yellow",
            "rulerColour": "yellow",
            "iconColour": "yellow"
        },
        "FIXME": {
            "background": "red",
            "icon": "beaker",
            "rulerColour": "red",
            "iconColour": "red",
        },
        "tag": {
            "background": "blue",
            "icon": "tag",
            "rulerColour": "blue",
            "iconColour": "blue",
            "rulerLane": "full"
        },
        "done": {
            "background": "green",
            "icon": "issue-closed",
            "rulerColour": "green",
            "iconColour": "green",
        },
        "note": {
            "background": "#f90",
            "icon": "note",
            "rulerColour": "#f90",
            "iconColour ": "#f90"
        }
    },
    "files.associations": {
        "*.json": "jsonc"
    },
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorBlinking": "solid",
    "window.menuBarVisibility": "default",
    "python.linting.pycodestyleCategorySeverity.W": "Error",
    "python.linting.flake8CategorySeverity.W": "Error",
    "editor.fontFamily": "Roboto Mono",
    "editor.quickSuggestionsDelay": 1,
    "editor.cursorWidth": 3,
    "editor.hover.delay": 100,
    "git.autofetch": true,
    "python.dataScience.allowImportFromNotebook": false,
    "python.dataScience.changeDirOnImportExport": false,
    "python.dataScience.searchForJupyter": false,
    "python.dataScience.showJupyterVariableExplorer": false,
    "python.dataScience.useDefaultConfigForJupyter": false,
    "python.formatting.provider": "yapf",
    "python.jediPath": "jedi",
    "python.jediMemoryLimit": 500,
    "markdownlint.run": "onSave",
    // jedi is good ;jediEnable:True
    // tag: add vscode setting here
    "leetcode.endpoint": "leetcode-cn",
    "leetcode.defaultLanguage": "python3",
    // leetcode abobe
    "nimble.timer": {
        "showReminders": "true",
        "series": [
            {
                "pattern": [
                    {
                        "type": "Work",
                        "time": "25"
                    },
                    {
                        "type": "Break",
                        "time": "5"
                    }
                ],
                "repeat": "8"
            }
        ]
    },
    // promo timer above
    "cmake.generator": "Visual Studio 14 2015",
    "cmake.platform": "x64",
    "cmake.toolset": "host=x64",
    "cmake.useCMakeServer": false,
    // cmake setting above
    "workbench.colorCustomizations": {
        "tab.activeBackground": "#f1e75369",
        "editorCursor.foreground": "#ebee3b"
    },
    // customiz color theme above
    "editor.tokenColorCustomizations": {
        // "functions": "#FF0000",
        // "comments": "#FF0000",
        // "variables": "bule",
        // "types": "#FF0000",
        // "textMateRules": [],
        // "strings": "#FF0000",
        // "numbers": "#FF0000",
        // "keywords": "#FF0000",
    },
    "python.linting.pycodestyleEnabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "dart.flutterSdkPath": "C:\\flutter",
    "dart.flutterCreateAndroidLanguage": "kotlin",
    "workbench.iconTheme": "vscode-icons",
    "breadcrumbs.enabled": false,
    "breadcrumbs.symbolPath": "off",
    // customized syntax color
    "editor.glyphMargin": true, // 编辑器侧边(断点)部分
    "workbench.tree.renderIndentGuides": "always",
    "autoDocstring.includeName": true,
    "autoDocstring.docstringFormat": "google",
    // tabnine setting
    "tabnine.disable_file_regex": [
        "*.md"
    ],
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "oneDarkPro.editorTheme": "Onedark Pro",
    "oneDarkPro.vivid": true,
    "oneDarkPro.italic": false,
    "oneDarkPro.bold": true,
    "leetcode.filePath": {
        "default": {
            // relative folder path
            "folder": "./",
            // file name
            "filename": "leetcode-${id}-${name}-${tag}-${difficulty}.${ext}"
        }
    },
    "leetcode.workspaceFolder": "/home/gl/Projects/algorithm-practise/gl-algorithm-practise/leetcode-gl-python/",
    "leetcode.hint.configWebviewMarkdown": false,
    "leetcode.showCommentDescription": true,
    "leetcode.hint.commandShortcut": false,
    "debug.allowBreakpointsEverywhere": true,
    "bookmarks.saveBookmarksInProject": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": true,
    // "terminal.integrated.fontFamily": "Roboto Mono"
    "python.jediPath": "/usr/lib/python3/dist-packages/jedi",
    "editor.formatOnSave": true,
    "python.formatting.yapfPath": "/home/gl/.local/bin/yapf",
    "latex-workshop.view.pdf.viewer": "tab"
}